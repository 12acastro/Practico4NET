name: dockercompose5037945769620771008
services:
  sqlserver:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Abc*123!
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: "1433"
      protocol: tcp
    volumes:
    - type: volume
      source: sqlserver-data
      target: /var/opt/mssql
      volume: {}
  webapi:
    build:
      context: C:\Users\MSI\Documents\Practico3NET
      dockerfile: WebAPI/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: webapi
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Users\MSI\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\MSI\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose5037945769620771008_default
volumes:
  sqlserver-data:
    name: dockercompose5037945769620771008_sqlserver-data